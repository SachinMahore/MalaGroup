                                <input type="text" id="txtBillingCity" class="span7" placeholder="Billing City" value="@Model.BillingCity" />
                                <input type="text" id="txtBillingState" class="span4" placeholder="Billing State" value="@Model.BillingState" />

                            </div>
                        </div>
