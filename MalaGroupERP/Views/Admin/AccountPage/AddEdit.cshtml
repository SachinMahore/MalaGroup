                    <button type="button" class="btn btn-primary" onclick="viewRefundOff();">Refund</button>
                    <button type="button" class="btn btn-primary" onclick="viewVoidOpp();">Void</button>
                    <button type="button" class="btn btn-danger" onclick="deleteAccount()">Delete</button>
                </div>
            </div>
        </div>
