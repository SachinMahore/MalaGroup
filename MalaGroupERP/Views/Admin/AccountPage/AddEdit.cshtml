                    <div class="control-group">
                        <div class="span2 pull-left">
                            <label class="control-label">Account Name</label>
                            <div class="controls">
                                <input type="text" id="txtAccountName" placeholder="Account name" value="@Model.AccountName" readonly />
                            </div>
                        </div>
                        <div class="span2">
                            <label class="control-label">Phone</label>
                            <div class="controls">
                                <input type="text" id="txtPhone" placeholder="Phone" value="@Model.Phone" />
                            </div>
                        </div>
                        <div class="span2">
                            <label class="control-label">Created By</label>

